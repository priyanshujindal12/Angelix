{
  "expo": {
    "name": "Angelix",
    "slug": "Angelix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "angelix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB6IK4gtfz-XGtTtz4qUjj7b3O8ohOeoNs"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.priyanshu1486.Angelix"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 280,
          "resizeMode": "contain",
          "backgroundColor": "#9333EA",
          "dark": {
            "backgroundColor": "#9333EA"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Angelix to use your location for safety tracking."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Angelix to access your camera for safety recording.",
          "microphonePermission": "Allow Angelix to record audio during emergencies."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      //"EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "pk_test_ZmlybS1nb2JsaW4tOTguY2xlcmsuYWNjb3VudHMuZGV2JA",
      "router": {},
      "eas": {
        "projectId": "8ddf1a28-8a39-4f5d-811f-da51d0342811"
      }
    }
  }
}